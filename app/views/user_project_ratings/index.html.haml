.block
  .secondary-navigation
    %ul.wat-cf
      %li.first.active= link_to "#{t("web-app-theme.list", :default => "List")}", user_project_ratings_path
      %li= link_to "#{t("web-app-theme.new", :default => "New")}", new_user_project_rating_path
  .content
    %h2.title
      = t("web-app-theme.all", :default => "All")
      UserProjectRatings
    .inner
      %table.table
        %tr
          %th.first ID
          %th
            = t("activerecord.attributes.user_project_rating._type", :default => t("activerecord.labels._type", :default => "_type"))
          %th= t("web-app-theme.created_at", :default => "Created at")
          %th.last &nbsp;
        - @user_project_ratings.each do |user_project_rating|
          %tr{:class => cycle("odd", "even")}
            %td
              = user_project_rating.id
            %td
              = link_to user_project_rating._type, user_project_rating_path(user_project_rating)
            %td
              = user_project_rating.created_at
            %td.last
              = link_to "#{t("web-app-theme.show", :default => "Show")}", user_project_rating_path(user_project_rating)
              |
              \#{link_to "#{t("web-app-theme.edit", :default => "Edit")}", edit_user_project_rating_path(user_project_rating)} |
              \#{link_to "#{t("web-app-theme.delete", :default => "Delete")}", user_project_rating_path(user_project_rating), :method => :delete, :confirm => "#{t("web-app-theme.confirm", :default => "Are you sure?")}"}
      .actions-bar.wat-cf
        .actions
- content_for :sidebar, render(:partial => 'sidebar')
